FROM nvidia/cuda:12.1.1-cudnn8-devel-ubuntu20.04


RUN apt-get update && apt-get install -y git libgl1 

RUN apt-get install -y python3-pip python3.10

COPY SD-ControlNet-TripoSR-main /SD-ControlNet-TripoSR-main

WORKDIR /SD-ControlNet-TripoSR-main




RUN pip3 install .



RUN pip install git+https://github.com/tatsy/torchmcubes.git





# 安装所需的依赖


# 在启动容器时运行gradio.py
#CMD ["python", "gradio.py"]

